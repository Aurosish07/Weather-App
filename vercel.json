{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/public/$1"
      },
      {
        "source": "/loc",
        "destination": "/index.js"
      },
      {
        "source": "/manual",
        "destination": "/index.js"
      }
    ],
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ]
  }
  